	journal = {Journal of Anxiety Disorders},
	author = {Heinig, Ingmar and Weiß, Martin and Hamm, Alfons O. and Hein, Grit and Hollandt, Maike and Hoyer, Jürgen and Kanske, Philipp and Richter, Jan and Wittchen, Hans-Ulrich and Pittig, Andre},
	month = jan,
	year = {2024},
	keywords = {Ecological momentary assessment, Physical activity, Anxiety disorders, Effectiveness of psychotherapy, Exposure therapy, Motor activity},
	pages = {102792},
	file = {Heinig et al_2024_Exposure traced in daily life.pdf:G\:\\Meine Ablage\\Zotero\\Heinig et al_2024_Exposure traced in daily life.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\UKWHome\\Zotero\\storage\\TJFFBKH3\\S0887618523001305.html:text/html},
